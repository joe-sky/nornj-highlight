{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "noUnusedParameters": false,
    "plugins": [
      {
      "name": "typescript-nornj-plugin",
      "tags": [
          "nj"
        ],
        "format": { "enabled": true }
      }
    ]
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
